<div class="main user-show">
  <div class="container">
    <div class="user">
      
        <img src="<%="/user_images/#{@user.image_name}"%>">
    
      <h2><%= @user.name %></h2>
      <p><%= @user.email %></p>
      <% if session[:user_id] == @user.id %>
        <%= link_to("Edit", "/users/#{@user.id}/edit") %>
      <% end %>
    </div>
  </div>
</div>